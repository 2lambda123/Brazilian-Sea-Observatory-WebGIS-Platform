python motuclient-python/motuclient.py --user fsato --pwd FabioCMEMS2018 --motu http://nrt.cmems-du.eu/motu-web/Motu --service-id GLOBAL_ANALYSIS_FORECAST_PHY_001_024-TDS --product-id global-analysis-forecast-phy-001-024-hourly-t-u-v-ssh --longitude-min -180 --longitude-max 179.9166717529297 --latitude-min -80 --latitude-max 90 --date-min "2019-02-27 08:30:00" --date-max "2019-03-08 23:30:00" --depth-min 0.493 --depth-max 0.4942 --variable uo --variable vo --out-dir --variable zos --out-dir /opt --out-name teste_2_uv_uo.nc